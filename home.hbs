{{!< default}}
    <iframe height='80px' width='100%' frameborder='0' allowTransparency='true' scrolling='auto' src='banner amparo.html'></iframe>    
<div class="fb-like" data-href="https://web.facebook.com/Covid19mex.net/?modal=admin_todo_tour" data-width="" data-layout="button_count" data-action="like" data-size="large" data-share="true"></div>
{{! The tag above means - insert everything in this file into the {body} of the default.hbs template }}
<div class="main-content-area">
 
 
    {{> homepage/featured-section}}
      
    <div class="container">
        <div class="row">
            <div class="col-lg-8">
                <div class="latest-post-section post-group">
                   
		    <div class="post-group-title">
                        <span>{{t "Ultimas Noticias"}}</span>
                    </div>
		  
                    {{#foreach posts visibility="all"}}
                    {{#if @first}}
                    <div class="large-post clearfix">
                        {{#if feature_image}}
                        <a href="{{url}}">
                            <div class="featured-image-wrap">
                                <div class="featured-image" style="background-image:url({{feature_image}});"></div>
                            </div>
                        </a>
                        {{/if}}
                        <div class="content-wrap{{^if feature_image}} no-image{{/if}}">
                            <h2 class="title h4"><a href="{{url}}">{{title}}</a></h2>
                            <time class="date" datetime="{{date format="YYYY-MM-DD"}}" title="{{date format="DD MMMM, YYYY"}}">{{date format="MMMM DD, YYYY"}}</time>
                            <div class="excerpt">{{excerpt words="40"}}</div>
                        </div>
                    </div>
                    {{/if}}
                    {{/foreach}}
                    <div class="row">
                        {{#foreach posts visibility="all"}}
                        {{^if @first}}
                        <div class="col-md-6 small-post">
                            {{#if feature_image}}
                            <a href="{{url}}">
                                <div class="featured-image-wrap">
                                    <div class="featured-image" style="background-image:url({{feature_image}});"></div>
                                </div>
                            </a>
                            {{/if}}
                            <div class="content-wrap{{^if feature_image}} no-image{{/if}}">
                                <h2 class="title h5"><a href="{{url}}">{{title}}</a></h2>
                                <time class="date" datetime="{{date format="YYYY-MM-DD"}}" title="{{date format="DD MMMM, YYYY"}}">{{date format="MMMM DD, YYYY"}}</time>
                            </div>
                        </div>
                        {{/if}}
                        {{/foreach}}
                    </div>
                </div>
            </div>
            {{> homepage/tag-section-1}}
            {{> homepage/tag-section-2}}
            {{> homepage/tag-section-3}}
            {{> homepage/tag-section-6}}
            {{> homepage/tag-section-5}}
        </div>	    
	</div>
   

</div>
